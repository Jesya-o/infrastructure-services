server {
	listen 80 default_server;
	server_name _;

	location / {
		uwsgi_pass localhost:5050;
		include uwsgi_params;
	}

	location /prometheus {
        proxy_pass http://localhost:9090/; 
    }

	location /graph {
		proxy_pass http://localhost:9090;
		proxy_set_header Host $host;
		proxy_set_header X-Real-IP $remote_addr;
	}
}
